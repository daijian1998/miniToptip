.info{
  background-color: rgb(39, 192, 238);
}
.error{
  background-color: red;
}
.warn{
  background-color: orange;
}
.position{
  position: fixed;
  color: white;
  align-items: center;
  justify-content: center;
  display: flex;
  /* margin-left: 15%; */
}
.normal{
  width: 100%;
  top: -50px;

}
.normal-active{
  width: 100%;

  animation: trans 2s ease;
  /* top: 0px;
  background-color: palegreen;
  transform:translateY(20px);
  transition-duration: 2s; */
  /* transition: all .9s linear; */
}
@keyframes trans{
  0% {margin-top: -10%;}
  30%  {margin-top: 0%;}
  50%  {margin-top: 0%;}
  100%  {margin-top: -10%;}
}
@keyframes trans2{
  0% {margin-top: 0%;}
  100%  {margin-top: -10%;}
}
@font-face {
  font-family: 'iconfont';
  src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;

  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-warn:before {
  content: "\e6a6";
  padding-right: 10px;
}

.icon-error:before {
  padding-right: 10px;
  content: "\e642";
}

.icon-info:before {
  padding-right: 10px;
  content: "\e727";
}
.tip{
  font-family: Arial, Helvetica, sans-serif;
}



